[PROCEDURE]
NAME Brw_EquiposJugadores
CATEGORY 'Browse'
[COMMON]
DESCRIPTION 'Browse the Equipos file'
FROM ABC Window
[DATA]
CurrentTab                STRING(80)
[FILES]
[PRIMARY]
Equipos
[INSTANCE]
1
[KEY]
Equ:EQU_Por_EquiposId
